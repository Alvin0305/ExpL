type
    bst {
        int value;
        bst left;
        bst right;
    }
endtype

decl
    bst root;
enddecl

int main() {
    decl
        bst left, right;
        bst leftleft, leftright, rightleft, rightright;
    enddecl

    begin
        root = alloc();
        left = alloc();
        right = alloc();
        leftleft = alloc();
        leftright = alloc();
        rightleft = alloc();
        rightright = alloc();

        root.left = left;
        left.left = leftleft;
        left.right = leftright;
        root.right = right;
        right.left = rightleft;
        right.right = rightright;

        root.value = 1;
        root.left.value = 2;
        root.left.left.value = 3;
        root.left.right.value = 4;
        right.value = 5;
        rightleft.value = 6;
        rightright.value = 7;

        write("From Root");
        write(root.value);
        write(root.left.value);
        write(root.left.left.value);
        write(root.left.right.value);
        write(root.right.value);
        write(root.right.left.value);
        write(root.right.right.value);

        write("From Node");
        write(root.value);
        write(left.value);
        write(leftleft.value);
        write(leftright.value);
        write(right.value);
        write(rightleft.value);
        write(rightright.value);
    end
}