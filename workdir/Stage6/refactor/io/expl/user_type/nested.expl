type
    C {
        int x;
    }
    
    B {
        C c;
    }

    A {
        B b;
    }
endtype

decl
    A a;
    int x;
enddecl

int main() {
    begin
        x = initialize();
        a = alloc();
        a.b = alloc();
        a.b.c = alloc();

        a.b.c.x = 777;
        write(a.b.c.x);
    end
}
