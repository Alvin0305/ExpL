type
    box {
        int value;
    }
endtype

decl
    box arr[5];
    int i, x;
enddecl

int main() {
    begin
        x = initialize();
        i = 0;
        while (i < 5) do
            arr[i] = alloc();
            arr[i].value = i * 10;
            i = i + 1;
        endwhile

        i = 0;
        while (i < 5) do
            write(arr[i].value);
            i = i + 1;
        endwhile
    end
}
