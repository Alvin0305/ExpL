begin
    decl
        int a[5], b[5];
        int i;
    enddecl

    i = 0;
    while(i < 5) do
        a[i] = (i + 1) * 2;
        i = i + 1;
    endwhile

    i = 0;
    while(i < 5) do
        b[i] = a[i];
        i = i + 1;
    endwhile

    i = 0;

    while(i < 5) do
        write(b[i]);
        i = i + 1;
    endwhile

end
